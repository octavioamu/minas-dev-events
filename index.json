{
  "$jason": {
    "head": {
      "title": "MINAS DEV events",
      "styles": {
        "note": {
          "color": "#E64C3B",
          "font": "HelveticaNeue-Bold",
          "size": "15"
        }
      },
      "actions": {
        "$pull": {
          "type": "$flush",
          "success": {
            "type": "$reload"
          }
        }
      },
      "data": {
        "events": [
          {
            "name": "FEMUG-MG"
          },
          {
            "name": "30ยบ Encontro do IxDA/BH"
          }
        ]
      },
      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "image",
              "style": {
                "width": "35",
                "height": "35"
              },
              "url": "https://raw.githubusercontent.com/octavioamu/minas-dev-events-app/master/images/minasdev-logo.png"
            }
          },
          "sections": [
            {
              "items": {
                "{{#each events}}": {

                  "components": [
                    {
                      "type": "vertical",
                      "style": {
                        "spacing": "5"
                      },
                      "components": [
                        {
                          "type": "label",
                          "text": "{{name}}",
                          "style": {
                            "font": "HelveticaNeue",
                            "size": "14"
                          }
                        }
                      ]
                    }
                  ]
                }
              }
            }
          ]
        }
      }
    }
  }
}